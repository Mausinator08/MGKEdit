{
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "recurse": false,
        "destination": "G:\\Workspaces\\MGKEdit\\docs"
    },
    "recurseDepth": 10,
    "source": {
        "include": [
            "G:\\Workspaces\\MGKEdit\\app\\MGKEdit\\src\\editor\\MGKEdit.js",
            "G:\\Workspaces\\MGKEdit\\app\\MGKEdit\\src\\editor-logic\\MGKEdit-Logic.js",
            "G:\\Workspaces\\MGKEdit\\app\\MGKEdit\\src\\main.js"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}