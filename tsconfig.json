{
    "compilerOptions": {
      "composite": true,
      "declaration": true,
      "declarationMap": true,
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "lib": [
        "es6",
        "dom"
      ],
      "noImplicitAny": true,
      "inlineSourceMap": true,
      "inlineSources": true,
      "outDir": "app",
      "baseUrl": ".",
      "paths": {
        "*": [
          "*",
          "node_modules/*",
          "../MGKFrameworkJS/*"
        ]
      },
      "rootDir": "../",
      "sourceRoot": "./../"
    },
    "extends": "../MGKFrameworkJS/tsconfig.base.json",
    "include": [
      "src/**/*",
      "../MGKFrameworkJS/src/**/*"
    ],
    "types": [
        "babylonjs"
    ]
}